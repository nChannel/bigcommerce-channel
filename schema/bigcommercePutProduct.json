      "order_quantity_maximum": {
        "type": "number"
      },
      "page_title": {
        "type": "string"
      },
      "meta_keywords": {
        "type": "array"
      },
      "meta_description": {
        "type": "string"
      },
      "view_count": {
        "type": "number"
      },
      "preorder_release_date": {
        "type": "string"
      },
      "preorder_message": {
        "type": "string"
      },
      "is_preorder_only": {
        "type": "boolean"

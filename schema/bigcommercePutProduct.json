          "key": {
            "type": "string"
          },
          "namespace": {
            "type": "string"
          },
          "value": {
            "type": [
              "string",
              "integer"
            ]
          },
          "description": {
            "type": "string"
          },
          "resource_type": {
            "type": "string",
            "enum": [
              "product",
              "variant",
              "category",
              "brand"
            ]
          }
        },
        "required": [
          "key",
          "namespace",
          "value",
          "permission_set"
        ],
        "additionalProperties": false
      }

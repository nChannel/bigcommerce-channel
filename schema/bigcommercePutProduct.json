        "enum": [
          "physical",
          "digital"
        ]
      },
      "sku": {
        "type": "string"
      },
      "description": {
        "type": "string"
      },
      "weight": {
        "type": "number"
      },
      "width": {
        "type": "number"
      },
      "depth": {
        "type": "number"
      },
      "height": {
        "type": "number"
      },
      "price": {
        "type": "number"
      },
      "cost_price": {
        "type": "number"
      },
      "retail_price": {
        "type": "number"
      },
      "sale_price": {
        "type": "number"
      },
      "tax_class_id": {
        "type": "number"
      },
      "product_tax_code": {
        "type": "string"
      },
      "categories": {
        "type": "array"
      },
      "brand_id": {
        "type": "number"
      },
      "inventory_level": {
        "type": "number"

          "is_customized": {
            "type": "boolean"
          }
        }
      },
      "open_graph_type": {
        "type": "string",
        "enum": [
          "product",
          "album",
          "book",
          "drink",
          "food",
          "game",
          "movie",
          "song",
          "tv_show"
        ]
      },
      "open_graph_title": {
        "type": "string"
      },
      "open_graph_description": {
        "type": "string"
      },
      "open_graph_use_meta_description": {
        "type": "boolean"
      },
      "open_graph_use_product_name": {
        "type": "boolean"
      },
      "open_graph_use_image": {
        "type": "boolean"
